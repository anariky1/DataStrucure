package ds.singlyLinkedList;

public class SinglyLinkedList {
		
	private Node first;
	
	public SinglyLinkedList(){
		
	}
	
	public boolean isEmpty(){
		return (first==null);
	}
	
	//used to insert at the begining of the list
	public void insertFirst(int data){
		Node newNode = new Node();
		newNode.data=data;
		newNode.next=first;
		
		
		
	}

}


