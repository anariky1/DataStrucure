package ds.circularlinkedlist;

public class CircularLinkedList {
	
	private Node first;
	private Node Last;
	
	public CircularLinkedList(){
		first=null;
		Last=null;
		
	}
	
	
	public void insertFirst(int data){
		Node newNode = new Node();
		newNode.data=data;
		
		if(isEmpty()){
			last=newNode;
		}
		
		newNode.next =first;
	}
	

}
