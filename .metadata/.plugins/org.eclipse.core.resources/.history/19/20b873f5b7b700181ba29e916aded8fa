package ds.doublylinkedlist;

public class DoublyLinkedList {
	
	private Node first;
	private Node last;
	
	public DoublyLinkedList(){
		first=null;
		last=null;
	}
	
	public boolean isEmpty(){
		return (first==null);
	}
	
	public void insertFirst(int data){
		Node newNode = new Node();
		newNode.data=data;
		
		if(isEmpty()){
			last=newNode;
		}
		
		newNode.next=first;
		first=newNode;
		
		
	}

}
